{
  "name": "vite-project",
  "private": true,
  "packageManager": "pnpm@latest",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build:app": "tsc && vite build",
    "build:image": "docker build .",
    "build": "pnmp run build:app && pnpm run build:image",
    "preview": "vite preview",
    "release": "semantic-release"
  },
  "repository": "https://github.com/DamianZamolski/frontend-pipeline",
  "release": {
    "branches": [
      "main"
    ]
  },
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "conventionalcommits"
      }
    ]
  ],
  "dependencies": {
    "react": "",
    "react-dom": ""
  },
  "devDependencies": {
    "@types/react": "",
    "@types/react-dom": "",
    "@vitejs/plugin-react": "",
    "semantic-release": "",
    "typescript": "",
    "vite": "",
    "vitest": "",
    "xo": ""
  }
}
